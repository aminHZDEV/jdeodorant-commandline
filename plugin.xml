<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
         <application>
         <run
               class="jdeodorant.eclipse.commandline.Application">
         </run>
         </application>
   </extension>
   <extension
         id="jdproduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="jdeodorant.eclipse.commandline-lambda.application"
            name="JDeodorant-Commandline-Product-Configuration">
         <property
               name="appName"
               value="JDeodorant-Commandline-Product-Configuration">
         </property>
      </product>
   </extension>

</plugin>
